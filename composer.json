{
    "name": "xqus/laravel-fuzz",
    "license": "MIT",
    "authors": [
        {
            "name": "Audun Larsen",
            "email": "larsen@xqus.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
          "xqus\\LaravelFuzz\\": "src/"
        }
    },
    "autoload-dev": {
      "psr-4": {
          "xqus\\LaravelFuzz\\Tests\\": "tests"
      }
    },
    "require": {
      "livewire/livewire": "^2.2.6"
    },

    "extra": {
      "laravel": {
          "providers": [
              "xqus\\LaravelFuzz\\LaravelFuzzServiceProvider"
          ]
      }
  },
    "require-dev": {
        "phpunit/phpunit": "^9.1@dev",
        "orchestra/testbench": "^5.1"
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit",
        "test-f": "vendor\\bin\\phpunit --filter"
  }
}
